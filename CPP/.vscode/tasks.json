{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cppbuild",
			"label": "C/C++: g++.exe build active file",
			"command": "C:\\MinGW\\bin\\g++.exe",
			"args": [
				"-fdiagnostics-color=always",
				"-g",
				"${workspaceFolder}\\main.cpp",
				"${workspaceFolder}\\tabu.cpp",
				"${workspaceFolder}\\MatrixGraph.cpp",
				"-o",
				"${fileDirname}\\tabu.exe"
			],
			"problemMatcher": [
				"$gcc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"detail": "compiler: C:\\MinGW\\bin\\g++.exe"
		},
		{
			"type": "cppbuild",
			"label": "release",
			"command": "C:\\MinGW\\bin\\g++.exe",
			"args": [
				"-fdiagnostics-color=always",
				"-O3",
				"${workspaceFolder}\\main.cpp",
				"${workspaceFolder}\\tabu.cpp",
				"${workspaceFolder}\\MatrixGraph.cpp",
				"-o",
				"${workspaceFolder}\\tabu_release.exe"
			],
			"problemMatcher": [
				"$gcc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"detail": "compiler: C:\\MinGW\\bin\\g++.exe"
		}
	]
}